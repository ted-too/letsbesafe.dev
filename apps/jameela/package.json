{
  "name": "jameela",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"pnpm run dev:css\" \"remix dev -p 3005\"",
    "prisma:generate": "pnpm exec prisma generate",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "deploy": "fly deploy --remote-only",
    "start": "remix-serve build",
    "start:prod": "pnpm exec prisma migrate deploy && pnpm run start"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@headlessui/react": "^1.7.0",
    "@prisma/client": "^4.3.1",
    "@remix-run/node": "^1.7.0",
    "@remix-run/react": "^1.7.0",
    "@remix-run/serve": "^1.7.0",
    "clsx": "^1.2.1",
    "fluent-ffmpeg": "^2.1.2",
    "framer-motion": "^7.2.1",
    "iconsax-react": "^0.0.8",
    "minipass": "^3.3.4",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "spin-delay": "^1.2.0",
    "tiny-invariant": "^1.2.0",
    "ytdl-core": "^4.11.2",
    "ytsr": "^3.8.0"
  },
  "devDependencies": {
    "prisma": "^4.3.1",
    "@remix-run/dev": "^1.7.0",
    "@remix-run/eslint-config": "^1.7.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  },
  "packageManager": "pnpm@7.11.0"
}
